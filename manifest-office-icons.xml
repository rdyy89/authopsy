  <IconUrl DefaultValue="https://static.office.net/media/office-ui-fabric-react-assets/office-icons/16/mail.svg"/>
  <HighResolutionIconUrl DefaultValue="https://static.office.net/media/office-ui-fabric-react-assets/office-icons/64/mail.svg"/>
  <SupportUrl DefaultValue="https://rdyy89.github.io/authopsy/"/>
  <AppDomains>
    <AppDomain>https://rdyy89.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://rdyy89.github.io/authopsy/"/>
        <RequestedHeight>400</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
  </Rule>
</OfficeApp>
